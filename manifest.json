{
  "manifest_version": 3,
  "name": "ChatGPT Message Collector",
  "version": "1.0",
  "description": "Add checkboxes and collect messages on ChatGPT.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["*://*/*"],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}
